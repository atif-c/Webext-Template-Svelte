{
    "manifest_version": 3,
    "name": "webext-template-svelte",
    "description": "A simple extension written in Svelte",
    "icons": {
        "16": "assets/logo/logo-16.png",
        "32": "assets/logo/logo-32.png",
        "48": "assets/logo/logo-48.png",
        "128": "assets/logo/logo-128.png"
    },
    "permissions": ["storage"],
    "action": {
        "default_title": "My Web Extension",
        "default_popup": "src/popup/popup.html"
    }
}
